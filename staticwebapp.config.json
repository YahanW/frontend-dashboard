{
    "routes": [
        {
            "route": "/profile",
            "redirect": "/profile",
            "statusCode": 301
        },
        {
            "route": "/dashboard",
            "serve": "/dashboard",
            "statusCode":200
        },
        {
            "route":"/login",
            "allowedRoles": ["anonymous"]
        }
    ],
      "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/"
    },
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/"
    },
    "404": {
      "rewrite": "/index.html"
    }
  },
}
